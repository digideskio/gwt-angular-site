<?xml version="1.0" encoding="UTF-8"?>
<module>
  <inherits name="com.google.gwt.user.User" />
  <inherits name="com.asayama.gwt.core.Core" />
  <inherits name="com.google.gwt.resources.Resources" />

  <generate-with class="com.asayama.gwt.angular.rebind.CreatorFactoryGenerator">
    <when-type-assignable class="com.asayama.gwt.angular.client.CreatorFactory" />
  </generate-with>
  <generate-with class="com.asayama.gwt.angular.rebind.DependenciesFactoryGenerator">
    <when-type-assignable class="com.asayama.gwt.angular.client.DependenciesFactory" />
  </generate-with>
  <generate-with class="com.asayama.gwt.angular.rebind.BinderFactoryGenerator">
    <when-type-assignable class="com.asayama.gwt.angular.client.BinderFactory" />
  </generate-with>
  <generate-with class="com.asayama.gwt.angular.rebind.ScopeBinderFactoryGenerator">
    <when-type-assignable class="com.asayama.gwt.angular.client.ScopeBinderFactory" />
  </generate-with>

  <generate-with class="com.asayama.gwt.angular.rebind.PartialsGenerator">
    <when-type-assignable class="com.asayama.gwt.angular.client.Partials" />
  </generate-with>

  <source path="client" />
  <entry-point class="com.asayama.gwt.angular.client.NG" />

</module>
