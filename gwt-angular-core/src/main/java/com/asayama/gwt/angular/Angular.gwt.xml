<?xml version="1.0" encoding="UTF-8"?>
<module>
  <inherits name="com.google.gwt.user.User" />
  <inherits name="com.asayama.gwt.core.Core" />
  <source path="client" />
  <source path="shared" />
  <generate-with class="com.asayama.gwt.angular.rebind.CreatorGenerator">
    <when-type-assignable class="com.asayama.gwt.angular.client.Creator" />
  </generate-with>
  <generate-with class="com.asayama.gwt.angular.rebind.DependenciesGenerator">
    <when-type-assignable class="com.asayama.gwt.angular.client.Dependencies" />
  </generate-with>
  <generate-with class="com.asayama.gwt.angular.rebind.BinderGenerator">
    <when-type-assignable class="com.asayama.gwt.angular.client.Binder" />
  </generate-with>
  <generate-with class="com.asayama.gwt.angular.rebind.InjectorGenerator">
    <when-type-assignable class="com.asayama.gwt.angular.client.Injector" />
  </generate-with>
</module>