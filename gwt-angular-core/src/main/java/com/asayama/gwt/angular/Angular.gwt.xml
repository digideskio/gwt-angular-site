<?xml version="1.0" encoding="UTF-8"?>
<module>
  <inherits name="com.google.gwt.user.User" />
  <inherits name="com.asayama.gwt.core.Core" />
  <script src="lib/angular/1.2.9/angular.min.js" />
  <generate-with class="com.asayama.gwt.angular.rebind.DependenciesFactoryGenerator">
    <when-type-assignable class="com.asayama.gwt.angular.client.DependenciesFactory" />
  </generate-with>
  <generate-with class="com.asayama.gwt.angular.rebind.BinderFactoryGenerator">
    <when-type-assignable class="com.asayama.gwt.angular.client.BinderFactory" />
  </generate-with>
  <generate-with class="com.asayama.gwt.angular.rebind.InjectorFactoryGenerator">
    <when-type-assignable class="com.asayama.gwt.angular.client.InjectorFactory" />
  </generate-with>
  <source path="client" />
  <source path="shared" />
  <entry-point class="com.asayama.gwt.angular.client.NG" />
</module>
