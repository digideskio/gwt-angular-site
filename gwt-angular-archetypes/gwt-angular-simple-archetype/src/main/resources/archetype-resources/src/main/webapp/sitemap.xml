#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
   <url>
      <loc>${symbol_dollar}{project.url}</loc>
      <lastmod>${symbol_dollar}{timestamp.yyyymmdd}</lastmod>
      <changefreq>daily</changefreq>
      <priority>0.8</priority>
   </url>
   <url>
      <loc>${symbol_dollar}{project.url}/${symbol_dollar}{project.version}${symbol_pound}!/jumbotron</loc>
      <lastmod>${symbol_dollar}{timestamp.yyyymmdd}</lastmod>
      <changefreq>weekly</changefreq>
      <priority>1.0</priority>
   </url>
</urlset>
